root = true

[*]
charset = utf-8
end_of_line = lf
indent_style = space
indent_size = 2
ij_typescript_force_quote_style = true
insert_final_newline = true
trim_trailing_whitespace = true
max_line_length = 120
quote_type = double
max_line_length = off

[*.md]
max_line_length = 0
trim_trailing_whitespace = false
